#!/usr/bin/execlineb -P

if -n {
  ls /etc/pulp/cert
}
foreground {
  mkdir -p /etc/pulp/cert
}
foreground {
  openssl ecparam -genkey -name prime256v1 -noout -out /etc/pulp/cert/token_private_key.pem
}
openssl ec -in /etc/pulp/cert/token_private_key.pem -pubout -out /etc/pulp/cert/token_public_key.pem
